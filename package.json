{
  "name": "ts-ed-example",
  "version": "0.1.0",
  "description": "Example for TS integration with Express.js using experimental decorators",
  "main": "index.js",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:build": "tsc -w",
    "start:run": "nodemon dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -r -f dist",
    "reinstall": "rm -r -f node-modules && rm package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethan-marsh/ts-ed-example.git"
  },
  "keywords": [
    "typescript",
    "express",
    "decorators"
  ],
  "author": "Ethans-mac-mini",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-session": "^2.0.37",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.1",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
